    <!-- Coverage tabs -->
    <div class="mb-4 border-b border-gray-200">
      <ul class="flex flex-wrap -mb-px" id="coverageTabs">
        <li class="mr-2">
          <button class="active-tab inline-block p-3 rounded-t-lg border-b-2 border-blue-600" id="basicTab" data-coverage="basic">
            Basic Coverage
